typedef bit<32> Ipv4Address;

header IPv4 {
    bit<4> version;
    bit<4> ihl;
    bit<8> diffserv;
    bit<16> totalLen;
    bit<16> identification;
    bit<3> flags;
    bit<13> fragOffset;
    bit<8> ttl;
    bit<8> protocol;
    bit<16> hdrChecksum;
    Ipv4Address srcAddr;
    Ipv4Address dstAddr;

}